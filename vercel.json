{
  "version": 2,
  "buildCommand": "npm install",
  "outputDirectory": "./",
  "includeFiles": [
    "views/**",
    "public/**",
    "config/**",
    "controllers/**",
    "middlewares/**",
    "models/**",
    "routes/**",
    "services/**",
    "utils/**",
    "app.js"
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
